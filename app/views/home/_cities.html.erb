<div class="row">
  <div class="well well-sm">
    <%= link_to 'Prefeituras', cities_path %>
    <div class="pull-right">
      <%= form_tag cities_path, :method => 'get' do %>
        <%= text_field_tag :search, params[:search] %>
        <%= submit_tag "Pesquisa", :name => nil %>
      <% end %>
    </div>
  </div>
  <% @managers.where(type_manager: 0).limit(3).each do |manager| %>
    <%= render 'manager', manager: manager %>
  <% end %>
</div>