<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @manager.name %>
</p>

<p>
  <strong>Election:</strong>
  <%= @manager.election.year %>
</p>

<p>
  <strong>Country:</strong>
  <%= @manager.country && @manager.country.name %>
</p>

<p>
  <strong>State:</strong>
  <%= @manager.state && @manager.state.name %>
</p>

<p>
  <strong>City:</strong>
  <%= @manager.city && @manager.city.name %>
</p>

<p>
  <strong>Type manager:</strong>
  <%= @manager.type_manager %>
</p>

<p>
  <strong>Start:</strong>
  <%= @manager.start %>
</p>

<p>
  <strong>End:</strong>
  <%= @manager.end %>
</p>

<p>
  <strong>User:</strong>
  <%= @manager.user.email %>
</p>

<%= link_to 'Promises', manager_promises_path(@manager), class: 'btn btn-primary' %> |
<%= link_to 'Edit', edit_manager_path(@manager), class: 'btn btn-primary' %> |
<%= link_to 'Back', managers_path, class: 'btn btn-primary' %>
